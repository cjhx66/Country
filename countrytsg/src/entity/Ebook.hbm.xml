<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="entity.Ebook" table="ebook">
		<id name="eid" column="eid" type="int">
			<generator class="native"/>
		</id>
		<property name="bname" column="bname"/>
		<property name="cid" column="cid"></property>
		<property name="url" column="url"></property>
		<property name="uid" column="uid"></property>
		<property name="time" column="time"></property>
		<property name="type" column="type"></property>
	</class>
</hibernate-mapping>