<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="entity.Menu" table="menu">
		<id name="menuId" column="menuId" type="int">
			<generator class="native"/>
		</id>
		<property name="menuName" column="menuName" />
		<property name="menuIcon" column="menuIcon" />
		<property name="pid" column="pid"/>
		<property name="menuUrl" column="menuurl"/>
		<property name="appid" column="appid"/>
	</class>
</hibernate-mapping>