<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.AppAuthority" table="appauthority">
		<id name="aid" column="aid" type="int">
			<generator class="native" />
		</id>
		<property name="rid" column="rid" />
		<property name="appid" column="appid" />
		<property name="menuids" column="menuids" />
		<property name="buttonids" column="buttonids" />
	</class>
</hibernate-mapping>